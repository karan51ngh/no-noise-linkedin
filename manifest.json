{
    "manifest_version": 3,
    "name": "NoNoise LinkedIn",
    "description": "NoNoise LinkedIn is a Free and Open Source extension that cleans up your LinkedIn feed by removing Suggested and Promoted posts.",
    "version": "1.3.0",
    "host_permissions": [
        "https://www.linkedin.com/"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/feed/"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}